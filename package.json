{
  "name": "nest-next-renderer",
  "description": "Render Next.js pages in Nest.js applications",
  "version": "2.1.0",
  "repository": "https://github.com/Gamote/nest-next-renderer.git",
  "author": "David Gamote <contact@gamote.ro>",
  "keywords": [
    "nest",
    "next",
    "renderer",
    "react",
    "react-dom",
    "nest-next",
    "fastify"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "index.ts",
    "index.d.ts",
    "index.js"
  ],
  "main": "dist/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "update-linked-project": "for file in `cat .linked.packages`; do cp -r dist $file; done",
    "dev": "nodemon -e ts --watch src --exec \"yarn build && yarn update-linked-project\"",
    "pack": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "publish": "yarn build && npm publish"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9",
    "@nestjs/core": ">=9",
    "fastify": ">=4.5",
    "next": ">=12"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify": "^4.5.3",
    "next": "^12.2.5",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
